<https://metadata.un.org/sdg/13>
    a skos:Concept, cids:Theme ;
    rdfs:isDefinedBy <https://codelist.commonapproach.org/codeLists/SDGImpacts> ;
    rdfs:label "Take urgent action to combat climate change and its impacts."@en ;
    skos:prefLabel "Goal 13: Climate action"@en ;
    skos:definition "Take urgent action to combat climate change and its impacts"@en ;
